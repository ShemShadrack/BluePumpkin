<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.capstone.spring.model.Winners" table="Winners" schema="" catalog="sums">
        <id name="winnerId">
            <column name="winnerId" sql-type="int" not-null="true"/>
        </id>
        <property name="winnerName">
            <column name="winnerName" sql-type="varchar" length="30" not-null="true"/>
        </property>
        <property name="eventName">
            <column name="eventName" sql-type="varchar" length="15" not-null="true"/>
        </property>
        <property name="prize">
            <column name="prize" sql-type="varchar" length="300" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>